DAY1:

problem 2:(prime numbers)

#include <stdio.h>

int main() {
	//code
	int t,i,j;
	scanf("%d",&t);
	for(i=0;i<t;i++)
	{
	   int a,k;
	   scanf("%d",&a);
	   for(j=2;j<=a;j++)
	   {
	       int flag=0;
	       for(k=j-1;k>=2;k--)
	       {
	           if(j%k==0)
	           {
	               flag=1;
	               break;
	           }
	       }
	       if(flag==0)
	       {
	           printf("%d ",j);
	       }
	   }
	   printf("\n");
	}
	return 0;
}

